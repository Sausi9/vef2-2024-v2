<%- include('header') -%>

<h1>Staðan í deildinni</h1>

<table>
  <thead>
    <tr>
      <th>Team</th>
      <th>Points</th>
    </tr>
  </thead>
  <tbody>
    <% standings.forEach(function(team) { %>
      <tr>
        <td><%= team.name %></td>
        <td><%= team.points %></td>
      </tr>
    <% }) %>
  </tbody>
</table>

<%- include('footer') -%>
